    "modal_max_width": "large",
    "file_icons": true,
  },
  "scrollbar": {
    "show": "never",
  },
  "minimap": {
    "show": "never",
  },
  "agent_buffer_font_size": 17,
  "cursor_blink": false,
  "cursor_shape": "bar",
  "multi_cursor_modifier": "alt",
  "selection_highlight": false,

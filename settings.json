        "function": {
          "font_style": "italic"
        },
        "function.call": {
          "font_style": "italic"
        },
        "variable": {
          "font_style": "italic"
        },
        "variable.parameter": {
          "font_style": "italic"
        },
        "variable.member": {
          "font_style": "italic"
        },
        "type": {
          "font_style": "italic"
        },
        "type.class": {
          "font_style": "italic"
        },
      },
    },

{
  "languages": {
    "Dart": {
      "colorize_brackets": true
    }
  },
  "buffer_font_family": "Maple Mono NF",
  "icon_theme": "Warm Charmed Icons",
  "ui_font_size": 17.0,
  "buffer_line_height": {
    "custom": 2
  },
  "buffer_font_features": {
    "cv01": 7
  },
  "ui_font_family": "Maple Mono NF",
  "buffer_font_size": 17.0,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Aura Dark",
  },
  "theme_overrides": {
    "Aura Dark": {
      "border.variant": "#15141C",
      "border": "#15141C",
      "title_bar.background": "#15141C",
      "panel.background": "#15141C",
      "panel.focused_border": "#4E466E",
      "players": [
        {
          "cursor": "#BD9DFF",
        },
      ],
      "syntax": {
        "comment": {
          "font_style": "italic",
        },
        "comment.doc": {
          "font_style": "italic"
        },
        "function": {
          "font_style": "italic"
        },
        "function.call": {
          "font_style": "italic"
        },
        "variable": {
          "font_style": "italic"
        },
        "variable.parameter": {
          "font_style": "italic"
        },
        "variable.member": {
          "font_style": "italic"
        },
        "type": {
          "font_style": "italic"
        },
        "type.class": {
          "font_style": "italic"
        },
      },
    },
  },
  "title_bar": {
    "show_onboarding_banner": false,
    "show_project_items": false,
    "show_branch_name": false,
    "show_user_menu": false,
  },
  "tab_bar": {
    "show": false,
  },
  "toolbar": {
    "quick_actions": false,
    "breadcrumbs": true,
    "agent_review": false,
  },
  "status_bar": {
    "experimental.show": true,
  },
  "gutter": {
    "min_line_number_digits": 0,
    "folds": false,
    "runnables": false
  },
  "indent_guides": {
    "enabled": false
  },
  "project_panel": {
    "dock": "right",
    "default_width": 300,
    "hide_root": true,
    "auto_fold_dirs": false,
    "starts_open": false,
    "git_status": false,
    "sticky_scroll": false,
    "scrollbar": {
      "show": "never",
    },
    "indent_guides": {
      "show": "never",
    },
  },
  "outline_panel": {
    "default_width": 300,
    "indent_guides": {
      "show": "never",
    },
  },
  "file_finder": {
    "modal_max_width": "large",
    "file_icons": true,
  },
  "scrollbar": {
    "show": "never",
  },
  "minimap": {
    "show": "never",
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "lsp": {
    "dart": {
      "binary": {
        "path": "C:/Users/parde/sdk/flutter/bin/dart.bat",
        "arguments": [
          "language-server",
          "--protocol=lsp"
        ]
      },
      "initialization_options": {
        "closingLabels": true,
        "outline": true,
        "flutterOutline": true
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy",
        },
      },
    },
  },
}

  },
  "buffer_font_features": {
    "cv01": 7
  },
  "ui_font_family": "Maple Mono NF",
  "buffer_font_size": 17.0,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Aura Dark",
  },
  "theme_overrides": {
    "Aura Dark": {
      "editor.document_highlight.read_background": "#00000000",

  "show_completion_documentation": true,
  "inline_code_actions": false,
  "lsp_document_colors": "inlay",
  "lsp": {
    "dart": {
      "binary": {
        "path": "C:/Users/parde/sdk/flutter/bin/dart.bat",
        "arguments": [
          "language-server",
          "--protocol=lsp"
        ]
      },
      "initialization_options": {
        "closingLabels": true,
        "outline": true,
        "flutterOutline": true
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy",
        },
      },
    },

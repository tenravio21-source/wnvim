    "breadcrumbs": true,
    "agent_review": false,
  },
  "status_bar": {
    "experimental.show": false,
  },
  "gutter": {
    "min_line_number_digits": 0,
    "folds": false,
    "runnables": false
  },
  "indent_guides": {
    "enabled": false
